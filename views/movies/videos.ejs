<!DOCTYPE html>
<html>
<head>
  <title>Videos</title>
</head>

<body>
  <h1>Videos for <%= movie.title %></h1>
  <% movie.videos.forEach(function(video) { %>
    <div>
      <h3>Video</h3>
      <video autoplay controls>
        <source src="<%= video %>" type="video/mp4">
      </video>
    </div>
  <% }) %>
</body>
</html>

